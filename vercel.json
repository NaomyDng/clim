{
  "redirects": [
    {
      "source": "http://climpro.be/:path*",
      "destination": "https://www.climpro.be/:path*",
      "permanent": true
    },
    {
      "source": "http://www.climpro.be/:path*",
      "destination": "https://www.climpro.be/:path*",
      "permanent": true
    },
    {
      "source": "https://climpro.be/:path*",
      "destination": "https://www.climpro.be/:path*",
      "permanent": true
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-store, no-cache, must-revalidate, max-age=0"
        }
      ]
    }
  ]
}
